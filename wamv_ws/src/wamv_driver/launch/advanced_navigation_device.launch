<?xml version="1.0"?>
<launch>

  <group ns='sensor/advanced_navigation_device'>

    <node pkg="advanced_navigation_driver" type="advanced_navigation_driver" name="driver" respawn="true">
        <!-- <param name='port' value='' /> -->
        <!-- <param name='baud_rate' value='' /> -->
        <param name='imu_frame_id' value='imu_raw' />
        <param name='nav_sat_frame_id' value='gps_raw' />
        <param name='topic_prefix' value='/sensor/advanced_navigation_device' />
        <remap from='/sensor/advanced_navigation_device/Twist' to='/input/twist/raw' />
        <remap from='/sensor/advanced_navigation_device/Imu' to='/input/imu/raw' />
        <remap from='/sensor/advanced_navigation_device/NavSatFix' to='/input/gps/raw' />
        <remap from='/sensor/advanced_navigation_device/SystemStatus' to='/sensor/advanced_navigation_device/system_status' />
        <remap from='/sensor/advanced_navigation_device/FilterStatus' to='/sensor/advanced_navigation_device/filter_status' />
    </node>

  </group>

</launch>
