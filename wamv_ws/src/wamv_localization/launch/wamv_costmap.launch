<?xml version="1.0"?>
<launch>
    <!-- load costmap node -->
    <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
        <rosparam file="$(find wamv_localization)/params/costmap_common.yaml" command="load" ns="global_costmap"/>
        <rosparam file="$(find wamv_localization)/params/costmap_common.yaml" command="load" ns="local_costmap"/>
        <rosparam file="$(find wamv_localization)/params/local_costmap_params.yaml" command="load"/>
        <rosparam file="$(find wamv_localization)/params/global_costmap_params.yaml" command="load"/>
        <remap from="odom" to="odometry/filtered"/>
    </node>
</launch>
