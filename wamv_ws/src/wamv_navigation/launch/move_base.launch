<?xml version="1.0"?>
<launch>

   <master auto="start"/>
 <!-- Run the map server -->
    <node pkg="map_server" type="map_server" name="map_server_node" args="$(find husky_navigation)/maps/playpen_map.yaml"/>

 <!--- Run AMCL -->
    <!-- <include file="$(find amcl)/examples/amcl_omni.launch" /> -->

   <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find wamv_navigation)/params/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find wamv_navigation)/params/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find wamv_navigation)/params/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find wamv_navigation)/params/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find wamv_navigation)/params/base_local_planner_params.yaml" command="load" />
 </node>

</launch>
